node {
    withMaven(maven:'maven') {

        stage('Checkout') {
       
          git url: 'https://github.com/acceval/ms-util.git', 
            credentialsId: 'c24eaaf9-1d02-4ab9-922c-e1a4e6973a44', 
            branch: 'master'
        }
        
        stage('Build') {
            
            sh 'mvn clean install'
        }
    }
}